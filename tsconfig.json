{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "module": "es2022",
    "target": "es2022",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strictFunctionTypes": false,
    "outDir": "./app/assets/builds"
  },
  "include": ["./app/frontend/**/*.ts", "./vite.config.ts"],
  "exclude": ["./node_modules"]
}
